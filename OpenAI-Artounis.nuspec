<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <!-- Package Name -->
    <id>OpenAI-Artounis</id>

    <!-- Package Version -->
    <version>1.0.0</version>

    <!-- Authors of the Package -->
    <authors>Artounis - Arash Seyedi</authors>

    <!-- Short Description of the Package -->
    <description>A C# client library for interacting with OpenAI's API, enabling access to models like GPT-3, completions, embeddings, fine-tuning, and more.</description>

    <!-- Project URL (Optional) -->
    <projectUrl>https://github.com/yourusername/OpenAIClient</projectUrl>

    <!-- Title of the Package -->
    <title>OpenAIClient</title>
    
    <!-- Tags for NuGet Search -->
    <tags>OpenAI API client</tags>
    
    <!-- License URL and License Type -->
    <licenseUrl>https://opensource.org/licenses/MIT</licenseUrl>
    <license type="expression">MIT</license>

    <!-- Package Dependencies -->
    <dependencies>
      <dependency id="Newtonsoft.Json" version="13.0.3" />
      <!-- Add other dependencies if needed -->
    </dependencies>
  </metadata>

  <!-- Files to be included in the package -->
  <files>
    <!-- Add DLL files from the release directory -->
    <file src="bin\Release\*.*" target="lib\netstandard2.0" />
    <!-- Add other files if needed -->
  </files>
</package>
