<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<!-- Package Name -->
		<id>Artounis-OpenAI-Client</id>

		<!-- Package Version -->
		<version>1.0.0</version>

		<!-- Authors of the Package -->
		<authors>Artounis</authors>

		<!-- Short Description of the Package -->
		<description>A C# client library for interacting with OpenAI's API, enabling access to models like GPT-3, completions, embeddings, fine-tuning, and more.</description>

		<!-- Project URL (Optional) -->
		<projectUrl>https://github.com/arashplus/OpenAI-Artounis</projectUrl>

		<!-- Title of the Package -->
		<title>OpenAIClient</title>

		<!-- Tags for NuGet Search -->
		<tags>OpenAI API client</tags>

		<!-- Package Dependencies -->
		<dependencies>
			<dependency id="Newtonsoft.Json" version="13.0.3" />
		</dependencies>

		<!-- License URL -->
		<licenseUrl>https://opensource.org/licenses/MIT</licenseUrl>
		<!-- Replace with actual license URL -->

		<!-- License expression -->
		<license type="expression">MIT</license>

		<!-- Repository URL -->
		<repositoryUrl>https://github.com/arashplus/OpenAI-Artounis</repositoryUrl>

		<!-- Authors -->
		<authors>Artounis</authors>

		<!-- Copyright Information -->
		<copyright>Copyright (c) 2023 Artounis</copyright>

		<!-- Icon URL -->
		<iconUrl>https://example.com/path/to/your/icon.png</iconUrl>
		<!-- Replace with actual icon URL -->
	</metadata>

	<!-- Files to be included in the package -->
	<files>
		<!-- Include LICENSE file -->
		<file src="licenses\LICENSE.txt" target="LICENSE.txt" />

		<!-- Include README file -->
		<file src="docs\readme.md" target="readme.md" />

		<!-- Add Icon -->
		<file src="images\icon.png" target="images\icon.png" />

		<!-- Add DLL files from the build directory -->
		<file src="bin\Release\netstandard2.0\*.*" target="lib\netstandard2.0" />

		<!-- Add other specific DLLs or files if needed -->
		<file src="bin\Debug\net9.0\OpenAI-Artounis.dll" target="lib\net9.0" />
	</files>
</package>
